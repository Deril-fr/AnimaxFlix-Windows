{
	"name": "sveltekit-electron",
	"version": "0.0.3",
	"private": true,
	"description": "Minimal Sveltekit + Electron starter template.",
	"main": "src/electron.cjs",
	"type": "module",
	"author": "Trail",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "vite dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"build:svelte": "vite build",
		"build:electron": "electron-builder --config build.config.json --publish=never"
	},
	"engines": {
		"npm": ">=7",
		"yarn": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385"
	},
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"@vime/core": "^5.3.3",
		"@vime/svelte": "^5.3.3",
		"axios": "^1.1.3",
		"buffer": "^6.0.3",
		"electron-context-menu": "^3.3.0",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"fuse-ts": "^0.2.2",
		"fuse.js": "^6.6.2",
		"html-to-text": "^8.2.1",
		"material-icons": "^1.12.0",
		"svelte-carousel": "^1.0.20",
		"svelte-local-storage-store": "^0.3.1",
		"svelte-navigator": "^3.2.2",
		"yrv": "^0.0.57"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "1.0.0-next.43",
		"@sveltejs/kit": "1.0.0-next.504",
		"@types/fuse": "^2.6.0",
		"@types/html-to-text": "^8.1.1",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"autoprefixer": "^10.4.12",
		"carousel-js": "^4.0.0",
		"concurrently": "^7.2.2",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.1",
		"electron": "^21.0.0",
		"electron-builder": "^23.1.0",
		"electron-connect": "^0.6.3",
		"electron-packager": "^16.0.0",
		"electron-updater": "^5.0.5",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.53.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.3"
	},
	"overrides": {
		"electron": {
			"got": "^12.5.1"
		}
	}
}
